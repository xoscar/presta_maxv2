{{#errors}}
	<div class="white-text animated bounceIn red chip"> 
		{{message}}
    <i class="close material-icons">close</i>
  </div>
{{/errors}}
{{#success}}
	<div class="white-text animated bounceIn green chip"> 
		{{message}}
    <i class="close material-icons">close</i>
  </div>
  <br>
{{/success}}